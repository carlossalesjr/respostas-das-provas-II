Q.3 Prova 2012
*******************************************************************************/
#include <stdio.h>

void main()
{
  int num, num1, num2, soma;
  
  for (num=1000; num<=9999; num++){
      num1= num/100;
      num2= num%100;
      soma= num1+num2;
      if (soma*soma == num)
      printf("%d ", num);
  }
}
